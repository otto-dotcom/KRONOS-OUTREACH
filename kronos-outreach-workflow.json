    {
      "parameters": {
        "jsCode": "// Follow-up Logic\nconst now = new Date();\nconst followupItems = [];\n\nfor (const item of $input.all()) {\n  const sentDate = new Date(item.json.sentDate);\n  const daysSinceSent = Math.floor((now - sentDate) / (1000 * 60 * 60 * 24));\n  \n  // Send follow-up after 3 days if no response\n  if (daysSinceSent >= 3 && daysSinceSent <= 4 && item.json.status === 'sent') {\n    item.json.followupSubject = `Re: ${item.json.subject}`;\n    item.json.followupBody = `Hi ${item.json.firstName},\\n\\nJust following up on my previous email.\\n\\nI understand you're busy, but I genuinely think we could help ${item.json.company} with [specific benefit].\\n\\nWould you have 10 minutes this week for a quick call?\\n\\nBest,\\n[YOUR NAME]`;\n    item.json.isFollowup = true;\n    followupItems.push(item);\n  }\n}\n\nreturn followupItems;"
      },
